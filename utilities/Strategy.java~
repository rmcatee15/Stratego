package utilities;
import utilities.Piece;
public class Strategy{
  public Piece[] setUpBoard(){
    Piece[] output = new Piece[30];
    output[0] = new Flag(33, 0, 0);
    output[1] = new Spotter(21, 0, 1);
    output[2] = new Spotter(21, 0, 2);
    output[3] = new Scout(22, 0, 3);
    output[4] = new Scout(22, 0, 4);
    output[5] = new Scout(22, 0, 5);
    output[6] = new Scout(22, 0, 6);
    output[7] = new Scout(22, 0, 7);
    output[8] = new Miner(23, 0, 8);
    output[9] = new Miner(23, 0, 9);
    output[10] = new Miner(23, 1, 0);
    output[11] = new Miner(23, 1, 1);
    output[12] = new Miner(23, 1, 2);
    output[13] = new Piece(24, 1, 3);//4
    output[14] = new Piece(24, 1, 4);//4
    output[15] = new Piece(25, 1, 5);//5
    output[16] = new Piece(25, 1, 6);//5
    output[17] = new Piece(26, 1, 7);//6
    output[18] = new Piece(26, 1, 8);//6
    output[19] = new Piece(27, 1, 9);//7
    output[20] = new Piece(28, 2, 0);//8
    output[21] = new Piece(29, 2, 1);//9
    output[22] = new Piece(30, 2, 2);//10
    output[23] = new Bomb(32, 2, 3);
    output[24] = new Bomb(32, 2, 4);
    output[25] = new Bomb(32, 2, 5);
    output[26] = new Bomb(32, 2, 6);
    output[27] = new Bomb(32, 2, 7);
    output[28] = new Bomb(32, 2, 8);
    output[29] = new Spy(31, 2, 9);
    return output;
  }
}